
{
	"name": "Slidev Dev Container",
	"build": {
		"dockerfile": "../Dockerfile",
		"context": ".."
	},
	"workspaceFolder": "/workspace",
	"workspaceMount": "source=${localWorkspaceFolder},target=/workspace,type=bind,consistency=cached",
	"forwardPorts": [3030],
	"remoteUser": "slide",
	"customizations": {
		"vscode": {
			"extensions": [
				"antfu.slidev",
				"vue.volar"
			]
		}
	}
}

